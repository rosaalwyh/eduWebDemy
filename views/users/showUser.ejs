<%- include('../layouts/header') %>
    <a href="/user/add" class="btn btn-info">Add</a>

  
    <div>
    <table class="table">
        <tr>
          <th>No</th>
          <th>Username</th>
          <th>Password</th>
          <th>Email</th>
          <th>Role</th>
          <th>Action</th>
          <th>Details User</th>
        </tr>
        <% dataUser.forEach((el, i) => { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= el.username %></td>
          <td><%= el.password %></td>
          <td><%= el.email %></td>
          <td><%= el.role %></td>
          <td>
              <a href="/user/edit/<%= el.id %>" class="btn btn-warning">Edit</a>
              <a href="/user/delete/<%= el.id %>" class="btn btn-danger">Delete</a>
          </td>
          <td>
            <a href="/detailUser/<%= el.id %>" class="btn btn-info"> See Details </a>
          </td>
        </tr>
        <% }) %>
      </table>      
<%- include('../layouts/footer') %>